{
  "name": "nodeapi",
  "version": "1.0.0",
  "description": "Examples of a Star Wars API",
  "main": "dist/server.js",
  "scripts": {
    "start": "tsc && node .",
    "debug": "export DEBUG=* && npm start",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitDylanHub/nodeAPI.git"
  },
  "keywords": [
    "API"
  ],
  "author": "Dylan Wright",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gitDylanHub/nodeAPI/issues"
  },
  "homepage": "https://github.com/gitDylanHub/nodeAPI#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "nodemon": "^2.0.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.16.0",
    "mocha": "^8.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
